{% extends "base.twig" %}

{% block content %}
	<div class="content-wrapper">
		<article class="page page-plugin post-type-{{ post.post_type }}" id="post-{{ post.ID }}">

			<div class="article__ce">
				{{content}}
			</div>
			
			<div class="article__sharer">
				{% include 'partial/sharer.twig' %}
			</div>

		</article>
	</div><!-- /content-wrapper -->
{% endblock %}
